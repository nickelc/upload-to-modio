{
    "name": "upload-to-modio",
    "version": "0.1.0",
    "description": "GitHub Action to upload mod files to mod.io",
    "main": "lib/main.js",
    "scripts": {
        "build": "tsc",
        "format": "prettier --write '**/*.ts'",
        "format:check": "prettier --check '**/*.ts'",
        "lint": "eslint 'src/**/*.ts'",
        "pack": "ncc build",
        "test": "jest",
        "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
    },
    "repository": {
        "type": "git",
        "url": "git+http://github.com/nickelc/upload-to-modio.git"
    },
    "keywords": [
        "actions",
        "modio",
        "upload",
        "mods"
    ],
    "author": "nickelc",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/nickelc/upload-to-modio/issues"
    },
    "homepage": "https://github.com/nickelc/upload-to-modio#readme",
    "dependencies": {
        "@actions/core": "^1.2.6",
        "axios": "^0.19.0",
        "form-data": "^3.0.0"
    },
    "devDependencies": {
        "@types/jest": "^26.0.0",
        "@types/node": "^14.0.13",
        "@typescript-eslint/parser": "^3.1.0",
        "@zeit/ncc": "^0.22.3",
        "eslint": "^7.2.0",
        "eslint-plugin-github": "^4.0.1",
        "eslint-plugin-jest": "^23.13.2",
        "jest": "^26.0.1",
        "jest-circus": "^26.0.1",
        "prettier": "^2.0.5",
        "ts-jest": "^26.1.0",
        "typescript": "^3.9.5",
        "typescript-formatter": "^7.2.2"
    }
}
